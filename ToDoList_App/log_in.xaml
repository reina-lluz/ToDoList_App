<Window x:Class="ToDoList_App.log_in"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ToDoList_App"
        mc:Ignorable="d"
        Title="log_in" Height="450" Width="800"
             ResizeMode="NoResize"
        WindowStartupLocation="CenterScreen" >
    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="/Log In.png" Stretch="UniformToFill" />
        </Grid.Background>

        <Button Content="Launch" Width="150" Height="50"
                FontSize="16" FontWeight="Bold"
                HorizontalAlignment="Center" VerticalAlignment="Top"
                Click="Launch_Click" Margin="0,229,0,0"/>
    </Grid>
</Window>